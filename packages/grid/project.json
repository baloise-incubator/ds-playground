{
  "name": "grid",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/grid/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/grid",
        "main": "packages/grid/src/index.ts",
        "tsConfig": "packages/grid/tsconfig.lib.json",
        "assets": [],
        "project": "packages/grid/package.json",
        "compiler": "swc",
        "format": ["cjs", "esm"]
      }
    },
    "publish": {
      "command": "node tools/scripts/publish.mjs grid {args.ver} {args.tag}",
      "dependsOn": ["build"]
    }
  },
  "tags": []
}
